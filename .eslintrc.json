{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": ["off", {
            "dependencies": true
        }],
        "no-extra-semi": "off",
        "no-trailing-spaces": "error",
        "import/extensions": "off",
        "no-console": "off"
    }
}
